        int Array[][]={
                {12,2,13,14},
                {1,2,3},
                {1,15},
        };

        int count = 1;
        for(int i =0; i < Array.length;i++){
            for(int k=0; k< Array[i].length;k++){
                if(Array[i][k]>=10){
System.out.println(Array[i][k]);
                }
            }
        }

    }
}
