public class Main {
    public static void main(String[] args) {
        String[][] Name = new String[5][5];

        Name[0][0] = "long";
        Name[0][1] = "Ani";
        Name[0][2] = "ANNA";
        Name[0][3] = "HTTP";
        Name[0][4] = "b";
        for (int i = 0; i < Name.length; ++i) {
            for (int j = 0; j < Name[i].length; j++) {
                if (i % 4 == 0) {
                    System.out.println(i + " ");
                    System.out.println(Name[0][4] = "b");
                }
                System.out.println();

            }


        }


    }
}
