           int Array[][]={
                {1,2,3,4},
                {1,2,3},
                {1,2},
        };

        int sum = 1;
        int a= 5;
        for(int i =0; i < Array.length;i++){
            for(int k=0; k< Array[i].length;k++){
                if(Array[i][k] % 2 == 0){
                    sum*=Array[i][k];
                }
            }
        }
        System.out.println(sum);
        System.out.println(2*4*2*2);
    }
}

